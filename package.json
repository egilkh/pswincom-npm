{
  "name": "pswincom-gateway-typed",
  "version": "1.4.0",
  "description": "Send SMS messages through LINK Mobility / PSWinCom Gateway, TypeScript version.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "tsx tests/index.spec.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "prettier:check": "prettier --check src/**/*",
    "prettier:write": "prettier --write src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apex-it/pswincom-npm-typed.git"
  },
  "keywords": [
    "SMS",
    "Gateway",
    "PSWinCom",
    "LINK Mobility"
  ],
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "xml2js": "^0.6.2"
  },
  "contributors": [
    {
      "name": "Henrik Grotle"
    },
    {
      "name": "Torbjørn Marø"
    },
    {
      "name": "Geir Gåsodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    },
    {
      "name": "Egil Hanger",
      "email": "egilkh@gmail.com",
      "url": "https://github.com/egilkh"
    }
  ],
  "license": "BSD",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@faker-js/faker": "^8.4.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.31",
    "@types/xml2js": "^0.4.14",
    "eslint": "^8.57.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0"
  },
  "engines": {
    "node": "^18"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSameLine": true,
    "arrowParens": "always",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
